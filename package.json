{
  "name": "@trionesdev/antd-react-ext",
  "version": "0.0.7-beta.4",
  "description": "A react library developed with dumi",
  "repository": {
    "url": "https://github.com/trionesdev/antd-react-ext.git"
  },
  "license": "MIT",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "deploy": "gh-pages -d dist",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && npm run build",
    "publish": "npm publish --registry=https://registry.npmjs.org/",
    "start": "npm run dev",
    "triones:publishOnly": "npm publish --ignore-scripts --registry=https://moensun-npm.pkg.coding.net/npm/moensun/",
    "version:beta": "npm version prerelease --preid beta"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0"
  },
  "devDependencies": {
    "@ant-design/cssinjs": "^1.21.1",
    "@ant-design/icons": "^5.3.7",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/lodash": "^4.17.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@umijs/lint": "^4.3.24",
    "antd": "^5.17.0",
    "cropperjs": "2.0.0-rc.2",
    "dumi": "^2.4.12",
    "eslint": "^8.46.0",
    "father": "^4.5.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "lodash": "^4.17.21",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^16.9.0"
  },
  "peerDependencies": {
    "@ant-design/cssinjs": "^1.21.1",
    "@ant-design/icons": "^5.3.6",
    "ahooks": "^3.8.1",
    "antd": "^5.17.0",
    "classnames": "^2.5.1",
    "cropperjs": "2.0.0-rc.2",
    "lodash": "^4.17.21",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "fengxiaotx@163.com"
  ]
}
